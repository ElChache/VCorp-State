{:source-paths ["src"]
 :dependencies [[reagent "1.2.0"]
                [re-frame "1.3.0"]
                [cljs-ajax "0.8.4"]
                [day8.re-frame/http-fx "0.2.4"]]
 
 :dev-http {#shadow/env ["FRONTEND_PORT" :as :int :default 3000] "public"}
 
 :builds
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"
        :modules {:main {:init-fn vcorpstate.core/init!}}
        :devtools {:http-root "public"
                   :http-port #shadow/env ["FRONTEND_PORT" :as :int :default 3000]}}}}